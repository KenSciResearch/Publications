  abstract={By observing how covert financial networks operate in online games like World of Warcraft, we can learn about how they might function offline.}
}

@article{shim2011analyzing,
  title={Analyzing human behavior from multiplayer online game logs: A knowledge discovery approach},
  author={Shim, Kyong Jin and Pathak, Nishith and Ahmad, Muhammad A and DeLong, Colin and Borbora, Zoheb and Mahapatra, Amogh and Srivastava, Jaideep},
