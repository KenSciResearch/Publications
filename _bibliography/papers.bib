  abstract={Observation and analysis of a phenomenon at unprecedented levels of granularity not only furthers our understanding of it, but also transforms the way it is studied. For instance, invention of gene-sequencing and computational analysis transformed the life sciences, creating fields of inquiry such as genomics, proteomics, etc., and the Hubble Space Telescope has furthered the ability of humanity to look much farther beyond what we could otherwise. }
}

@inproceedings{ahmad2011item,
  title={Item recommendations in multiple overlapping social networks in MMOs},
  author={Ahmad, Muhammad Aurangzeb and Srivastava, J},
  booktitle={The Third ACM WebSci Conference, Koblenz, Germany June},
  pages={14--17},
  year={2011},
  gaming={1},
  linkprediction={1},
  myconf={1},
  abbr={WebSci},
  pub_type={Paper},
  bibtex_show={},
  pdf={AhmadWebSciItems2011.pdf},
  pptx={AhmadWebSciItemsSlides2011.pdf},
  abstract={Item recommendation is a well studied problem and has been studied in a variety of contexts. The goal of recommendation  tasks is to recommend items or products to people in a rec- ommendation system such that these people make a positive  decision based on those recommendations. The advantange of using social networks for recommendations is that they  greatly reduce the search space of people for whom simi- laritty has to be computed to make recommendations. In  this paper we address this problem in the context of coex- tensive social networks i.e., multiple social networks with  overlapping membership and evolving in time. We use data from Massively Multiplayer Online Game (MMOG) for our  experiments. We get the surprising results that adversar- ial networks are most effective in the recommendation task  and the trust networks are not as effective in prediction as  compared to other networks. Additionally the choice of net- works also depends upon the type of items i.e., high end or  low end items for which recommendation has to be made.}
}

